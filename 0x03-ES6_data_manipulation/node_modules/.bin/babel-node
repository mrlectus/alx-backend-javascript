#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules/@babel/node/bin/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules/@babel/node/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules/@babel/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules/@babel/node/bin/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules/@babel/node/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules/@babel/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/@babel+node@7.22.19_@babel+core@7.23.0/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/node/bin/babel-node.js" "$@"
else
  exec node  "$basedir/../@babel/node/bin/babel-node.js" "$@"
fi
