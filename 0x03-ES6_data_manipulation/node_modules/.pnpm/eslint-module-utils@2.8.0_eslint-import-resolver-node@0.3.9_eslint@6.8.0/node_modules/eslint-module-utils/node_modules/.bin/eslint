#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/bin/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/eslint@6.8.0/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/bin/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/eslint@6.8.0/node_modules/eslint/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/eslint@6.8.0/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@6.8.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@6.8.0/node_modules/eslint/bin/eslint.js" "$@"
fi
