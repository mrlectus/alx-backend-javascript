#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/brown/Code/javascript/alx-backend-javascript/0x03-ES6_data_manipulation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jsesc" "$@"
else
  exec node  "$basedir/../../bin/jsesc" "$@"
fi
